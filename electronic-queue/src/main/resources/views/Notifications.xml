<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<object-views xmlns="http://axelor.com/xml/ns/object-views"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="http://axelor.com/xml/ns/object-views http://axelor.com/xml/ns/object-views/object-views_6.1.xsd">
  <grid title="Notifications" name="notifications-grid"
    model="com.axelor.apps.eq.db.Notifications">
    <field name="booking" title="Booking"/>
    <field name="type" title="Type" widget="singleWidget"/>
    <field name="message" title="Message"/>
    <field name="status" title="Status" widget="SingleWidget"/>
    <field name="sent_at" title="SentAt"/>
  </grid>

  <form title="Notifications" name="notifications-form"
    model="com.axelor.apps.eq.db.Notifications" width="large">
    <panel name="overviewPanel" title="Notifications Information">
      <field name="booking" title="Booking"/>
      <field name="type" title="Type" widget="single_widget"/>
      <field name="message" title="Message"/>
      <field name="status" title="Status" widget="single_widget"/>
      <field name="sent_at" title="SentAt"/>
    </panel>
  </form>
</object-views>