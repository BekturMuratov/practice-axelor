<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<object-views xmlns="http://axelor.com/xml/ns/object-views"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="http://axelor.com/xml/ns/object-views http://axelor.com/xml/ns/object-views/object-views_6.1.xsd">
  <grid title="BorderPoints" name="border-points-grid"
    model="com.axelor.apps.eq.db.BorderPoints">
    <field name="name" title="Name"/>
    <field name="location" title="Location"/>
    <field name="capacity" title="Capacity"/>
    <field name="is_active" title="Is Active" widget="SingleWidget"/>
  </grid>

  <form title="Border Points Form" name="border-points-form"
    model="com.axelor.apps.eq.db.BorderPoints" width="large">
    <panel name="overviewPanel" title="Border Points Information">
      <field name="name" title="Name"/>
      <field name="location" title="Location"/>
      <field name="capacity" title="Capacity"/>
      <field name="is_active" title="Is Active" widget="SingleWidget"/>
    </panel>
  </form>
</object-views>