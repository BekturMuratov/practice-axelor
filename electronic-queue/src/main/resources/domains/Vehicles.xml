<?xml version="1.0" encoding="UTF-8"?>
<domain-models xmlns="http://axelor.com/xml/ns/domain-models"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="http://axelor.com/xml/ns/domain-models http://axelor.com/xml/ns/domain-models/domain-models_6.1.xsd">
  <module name="eq" package="com.axelor.apps.eq.db"/>
  <entity name="Vehicles">
    <string name="driver_name" title="Driver Name"/>
    <string name="plate_number" title="Plate Number"/>
    <string name="trailer_number" title="Trailer Number"/>
    <string name="cargo_type" title="Cargo Type" default="normal"
      selection="select.vehicles.cargo_type.select"/>
    <string name="priority" title="Priority" selection="select.vehicles.priority.select"/>
    <datetime name="created_at" title="Created At"/>


    <finder-method name="findByPlateNumber" using="plate_number"/>
    <finder-method name="findByDriverId" using="driver_name"/>

  </entity>

</domain-models>