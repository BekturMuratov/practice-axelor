<?xml version="1.0" encoding="UTF-8"?>
<domain-models xmlns="http://axelor.com/xml/ns/domain-models"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="http://axelor.com/xml/ns/domain-models http://axelor.com/xml/ns/domain-models/domain-models_6.1.xsd">
  <module name="eq" package="com.axelor.apps.eq.db"/>
  <entity name="Notifications">
    <one-to-one name="booking" title="Booking" ref="com.axelor.apps.eq.db.Bookings"/>
    <string name="type" title="Type" selection="select.notifications.type.select"/>
    <string name="message" title="Message"/>
    <string name="status" title="Status" selection="select.notification.status.select"
      default="pending"/>
    <datetime name="sent_at" title="Sent At"/>
  </entity>
</domain-models>
