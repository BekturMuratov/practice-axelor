<?xml version="1.0" encoding="UTF-8"?>
<domain-models xmlns="http://axelor.com/xml/ns/domain-models"
               xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
               xsi:schemaLocation="http://axelor.com/xml/ns/domain-models http://axelor.com/xml/ns/domain-models/domain-models_6.1.xsd">

    <module name="SVH" package="com.axelor.apps.svh.db"/>

    <entity name="Tariff">
        <string name="name" title="name"/>
        <datetime name="startDateTime" title="startDateTime"/>
        <datetime name="endDateTime" title="endDateTime"/>
        <decimal name="price_per_day" title="price_per_day"/>
        <string name="transport_type" selection="svh.registration.transport_types_selections"/>
        <decimal name="min_weight" title="min_weight"/>
        <decimal name="max_weight" title="max_weight"/>
    </entity>


</domain-models>
