<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<object-views xmlns="http://axelor.com/xml/ns/object-views"
              xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
              xsi:schemaLocation="http://axelor.com/xml/ns/object-views http://axelor.com/xml/ns/object-views/object-views_6.1.xsd">

    <grid title="Services" name="services-grid" model="com.axelor.apps.svh.db.Services">
        <field name="name_of_service" title="Name of Service" />
        <field name="price_for_service" title="Price for Service" />
        <field name="amount" title="Amount" />
    </grid>

    <form title="Services" name="services-form" model="com.axelor.apps.svh.db.Services" width="large">
        <panel>
            <field name="name_of_service" title="Name of Service" />
            <field name="price_for_service" title="Price for Service" />
            <field name="amount" title="Amount" />
        </panel>
    </form>

</object-views>
