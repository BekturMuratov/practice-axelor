<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<object-views xmlns="http://axelor.com/xml/ns/object-views"
              xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
              xsi:schemaLocation="http://axelor.com/xml/ns/object-views http://axelor.com/xml/ns/object-views/object-views_6.1.xsd">
    <grid title="Tariffs" name="tariffs-grid" model="com.axelor.apps.svh.db.Tariffs">
        <field name="name" />
        <field name="start_date" />
        <field name="end_date" />
        <field name="price" />
        <field name="entry_type" />
    </grid>

    <form title="Tariffs Form" name="tariffs-form" width="large" model="com.axelor.apps.svh.db.Tariffs">
        <panel name="TariffsPanel" title="Tariff Panel" >
            <field name="name" />
            <field name="price" />
        </panel>
        <panel>
            <field name="start_date" />
            <field name="end_date" />
            <field name="entry_type" />
        </panel>
    </form>
</object-views>