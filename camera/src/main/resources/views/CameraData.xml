<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<object-views xmlns="http://axelor.com/xml/ns/object-views"
              xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
              xsi:schemaLocation="http://axelor.com/xml/ns/object-views http://axelor.com/xml/ns/object-views/object-views_6.1.xsd">

    <grid title="Camera Data" name="camera-grid" model="com.axelor.apps.camera.db.CameraData">
        <toolbar>
            <button name="get data" title="getData" onClick="startListener" />
        </toolbar>

        <field name="crossRecordSysCode" title="Cross Record System Code" />
        <field name="cameraIndexCode" title="Camera Index Code" />
        <field name="ownerName" title="Owner Name" />
        <field name="contact" title="Contact" />
        <field name="vehiclePicUri" title="VehiclePicUri" />
        <field name="crossTime" title="Cross Time" />
        <field name="vehicleColor" title="Vehicle Color" />
        <field name="vehicleType" title="Vehicle Type" />
        <field name="country" title="Country" />
        <field name="vehicleDirectionType" title="Vehicle Direction Type" />
        <field name="regionName" title="Region Name" />
        <field name="vehiclePicPath" title="Vehicle Pic Path" />
    </grid>

    <form title="Camera Data Form" name="camera-form" model="com.axelor.apps.camera.db.CameraData">
        <panel name="Overview Panel" title="type camera data">
            <field name="crossRecordSysCode" title="Cross Record System Code" />
            <field name="cameraIndexCode" title="Camera Index Code" />
            <field name="ownerName" title="Owner Name" />
            <field name="contact" title="Contact" />
            <field name="vehiclePicUri" title="VehiclePicUri" />
            <field name="crossTime" title="Cross Time" />
            <field name="vehicleColor" title="Vehicle Color" />
            <field name="vehicleType" title="Vehicle Type" />
            <field name="country" title="Country" />
            <field name="vehicleDirectionType" title="Vehicle Direction Type" />
            <field name="regionName" title="Region Name" />
            <field name="vehiclePicPath" title="Vehicle Pic Path" />
            <button name="get data" onClick="startListener" />
        </panel>
    </form>

    <action-method name="startListener">
        <call class="com.axelor.apps.camera.web.CameraDataController" method="startListener"/>
    </action-method>
</object-views>