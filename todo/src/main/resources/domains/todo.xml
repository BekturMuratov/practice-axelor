<?xml version="1.0" encoding="UTF-8"?>
<domain-models xmlns="http://axelor.com/xml/ns/domain-models"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="http://axelor.com/xml/ns/domain-models https://axelor.com/xml/ns/domain-models/domain-models_8.0.xsd">

  <module name="todo" package="com.axelor.todo.model"/>

  <entity name="Task">
    <string name="name" required="true"/>
    <string name="description"/>
    <string name="created_at" default="now"/>
    <string name="updated_at"/>
    <string name="status" selection="select.task.select"/>
  </entity>
</domain-models>
